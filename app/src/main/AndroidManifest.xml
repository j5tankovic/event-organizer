<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rs.ac.uns.ftn.pma.event_organizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyACF0eBzPtMZIbnjc0hAcnt3cnHh0L6VjQ" />

        <activity android:name=".UserProfileActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".PlaceOfferOverviewActivity"
            android:parentActivityName=".EventActivity" />
        <activity
            android:name=".ShoppingItemOverviewActivity"
            android:parentActivityName=".EventActivity" />
        <activity
            android:name=".NewPlaceOfferActivity"
            android:label="New place offer"
            android:parentActivityName=".EventActivity" />
        <activity
            android:name=".NewShoppingItemActivity"
            android:label="New shopping item"
            android:parentActivityName=".EventActivity" />
        <activity
            android:name=".EditPlaceOfferActivity"
            android:parentActivityName=".PlaceOfferOverviewActivity" />
        <activity
            android:name=".EditShoppingItemActivity"
            android:parentActivityName=".ShoppingItemOverviewActivity" />
        <activity android:name=".RegisterActivity"></activity>
    </application>

</manifest>